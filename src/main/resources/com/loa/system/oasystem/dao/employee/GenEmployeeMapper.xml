  <select id="findByEmployeeList" parameterType="map" resultMap="EmployeeVo">
    SELECT
      e.empid,
      e.ename,
      e.sex,
      e.realname,
      d.deptname,
      p.pname,
      e.hiredate,
      e.birthdate,
      e.leavedate,
      e.onduty,
      mgr.ename AS mgrname,
      e.phone,
      e.emercontactperson,
      e.idcard
    FROM
      employee e
    INNER JOIN
      dept d
    ON e.deptno = d.deptno
    INNER JOIN
      `position` p
    ON e.posid = p.posid
    LEFT JOIN
      employee mgr
    ON e.mgrid = mgr.empid
    <where>
      <if test="ename!=null">
        and ename = #{ename}
      </if>
      <if test="deptno!=null">
        and deptno = #{deptno}
      </if>
      <if test="onduty!=null">
        and onduty = #{onduty}
      </if>
      <if test="hiredate!=null">
        and hiredate = #{hiredate}
      </if>
    </where>
    order by p.posid
  </select>
